      %label.leave
        %select.GAMU4UJCB5B.GAMU4UJCJ3B.GAMU4UJCEI
          %option{:value => "1"} Leave now
          %option{:value => "0"} Leave at
      &nbsp;
      %input{type:'text', id:'setTime'}
    .form-action
      %input{type:'button', value:'clear', class:'btn btn-default', id:'clear_btn'}
      &nbsp;
      %input{type:'button', value:'route', class:'btn btn-success', id:'search_btn'}

  #direction_result_wrapper{style:'display:none'}
    =render 'home/routeheader'
    %ul#directions_mode.m_train
      %li#bus
        %span bus
      %li#train
        %span.active_mode train/bus
      %li#taxi
        %span taxi
      %li#drive
        %span drive

    %ul#myTabs.nav.nav-tabs{:role => "tablist"}
      %li.active{:role => "presentation"}
        %a#sooner-tab{"aria-controls" => "home", "aria-expanded" => "true", "data-toggle" => "tab", :href => "#sooner", :role => "tab"} Sooner
      %li{:role => "presentation"}
        %a#cheaper-tab{"aria-controls" => "profile", "aria-expanded" => "false", "data-toggle" => "tab", :href => "#cheaper", :role => "tab"} Cheaper

  #myTabContent.tab-content{style:'display:none'}
    #sooner.tab-pane.fade.active.in{"aria-labelledby" => "sooner-tab", :role => "tabpanel"}
      %p To show the fastest routes
      %div#fast_route_transit_info
        =render "home/fastest_transit_info"
    #cheaper.tab-pane.fade{"aria-labelledby" => "cheaper-tab", :role => "tabpanel"}
      %p To show the cheapest routes
      %div#cheap_route_transit_info
        =render "home/cheapest_transit_info"


    %table.GAMU4UJCBKC{:cellpadding => "0", :cellspacing => "0"}
      %tbody
        %tr
          %td{:align => "left", :style => "vertical-align: top;"}

        %tr
          %td{:align => "left", :style => "vertical-align: top;"}



